{
    "src":[
        "./index.js",
        "./src/*.js",
        "./src/**/*.js",
        "./README.md"
    ],
    "dest":"./docs",
    "jsdoc":{
        "hierarchy":true,
        "sort":"kind",
        "plugins":[
            "plugins/markdown"
        ]
    },
    "markdown":{
        "sanitize":false
    },
    "app":{
        "title":"Saratoga | An open source Azur Lane local library",
        "routing":"query",
        "entrance":"content:readme",
        "base":"/Saratoga"
    },
    "template":{
        "path":"zebra",
        "options":{
            "title":{
                "label":"Saratoga",
                "href":"."
            },
            "sidebar":{
                "enabled":true,
                "outline":"tree",
                "collapsed":false,
                "toolbar":true,
                "itemsFolded":false,
                "itemsOverflow":"crop",
                "badges":true,
                "search":true,
                "animations":true
            },
            "symbols":{
                "autoLink":true,
                "params":"list",
                "enums":"list",
                "props":"list",
                "meta":false
            },
            "navbar":{
                "enabled":true,
                "dark":false,
                "animations":true,
                "menu":[
                    {
                        "label":"Readme",
                        "href":"."
                    },
                    {
                        "label":"Documentation",
                        "href":"?api"
                    },
                    {
                        "label":"GitHub",
                        "href":"https://github.com/AzurAPI/Saratoga"
                    }
                ]
            }
        }
    }
}
